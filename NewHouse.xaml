<Window x:Class="SVPP_CS_WPF_Lab7_Characteristics_houses_Db_.NewHouse"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SVPP_CS_WPF_Lab7_Characteristics_houses_Db_"
        mc:Ignorable="d"
        Title="NewHouse" Height="400" Width="400"
        ResizeMode="NoResize">
    <StackPanel Name="StackPanel_NewHousing" Background="DarkGray">
        <local:HouseViewUserControl x:Name="HouseView" Margin="5"/>
        
        <StackPanel Name="StackPanel_Buttons" Orientation="Horizontal">
            
            <Button Name="Bt_Save" Content="Сохранить"
                    Margin="90 10 10 10" Padding="5"                   
                    Width="100" Height="35"/>
                    
            <Button Name="Bt_Cancel" Content="Отмена"
                    Margin="20 10 10 10" Padding="5"                   
                    Width="100" Height="35"/>
            
        </StackPanel>
    </StackPanel>
</Window>
